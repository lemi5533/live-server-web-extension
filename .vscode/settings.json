        "<!DOCTYPE html> <html lang=\"en\"> <head>   <meta charset=\"UTF-8\" />   <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\" />   <title>Ethiopian University Directory</title>   <style>     body {       font-family: Arial, sans-serif;       margin: 0;       padding: 20px;       background: #f4f4f4;     }      h1 {       text-align: center;       color: #2c3e50;     }      .search-box {       margin: 20px auto;       text-align: center;     }      input, select {       padding: 10px;       width: 80%;       max-width: 400px;       margin-bottom: 20px;       border: 1px solid #ccc;       border-radius: 8px;     }      .card {       background: white;       border-radius: 10px;       box-shadow: 0 2px 8px rgba(0,0,0,0.1);       padding: 15px;       margin: 10px auto;       max-width: 600px;     }      .card h2 {       margin: 0 0 10px;       color: #333;     }      .card p {       margin: 5px 0;       color: #666;     }   </style> </head> <body>   <h1>Ethiopian University Directory</h1>    <div class=\"search-box\">     <input type=\"text\" id=\"searchInput\" placeholder=\"Search universities by name...\" />     <br>     <select id=\"regionFilter\">       <option value=\"\">Filter by region</option>       <option value=\"Addis Ababa\">Addis Ababa</option>       <option value=\"Oromia\">Oromia</option>       <option value=\"Amhara\">Amhara</option>       <option value=\"Tigray\">Tigray</option>       <option value=\"SNNPR\">SNNPR</option>     </select>   </div>    <div id=\"universityList\"></div>    <script>     const universities = [       { name: \"Addis Ababa University\", region: \"Addis Ababa\", website: \"https://www.aau.edu.et\" },       { name: \"Adama Science and Technology University\", region: \"Oromia\", website: \"https://www.astu.edu.et\" },       { name: \"Bahir Dar University\", region: \"Amhara\", website: \"https://www.bdu.edu.et\" },       { name: \"Mekelle University\", region: \"Tigray\", website: \"https://www.mu.edu.et\" },       { name: \"Hawassa University\", region: \"SNNPR\", website: \"https://www.hu.edu.et\" },     ];      const listContainer = document.getElementById(\"universityList\");     const searchInput = document.getElementById(\"searchInput\");     const regionFilter = document.getElementById(\"regionFilter\");      function displayUniversities(filterText = \"\", region = \"\") {       listContainer.innerHTML = \"\";       const filtered = universities.filter(u => {         return u.name.toLowerCase().includes(filterText.toLowerCase()) &&                (region === \"\" || u.region === region);       });        if (filtered.length === 0) {         listContainer.innerHTML = \"<p>No universities found.</p>\";         return;       }        filtered.forEach(u => {         const card = document.createElement(\"div\");         card.className = \"card\";         card.innerHTML = `           <h2>${u.name}</h2>           <p><strong>Region:</strong> ${u.region}</p>           <p><a href=\"${u.website}\" target=\"_blank\">${u.website}</a></p>         `;         listContainer.appendChild(card);       });     }      searchInput.addEventListener(\"input\", () => {       displayUniversities(searchInput.value, regionFilter.value);     });      regionFilter.addEventListener(\"change\", () => {       displayUniversities(searchInput.value, regionFilter.value);     });      // Initial display     displayUniversities();   </script> </body> </html>"
    ],
